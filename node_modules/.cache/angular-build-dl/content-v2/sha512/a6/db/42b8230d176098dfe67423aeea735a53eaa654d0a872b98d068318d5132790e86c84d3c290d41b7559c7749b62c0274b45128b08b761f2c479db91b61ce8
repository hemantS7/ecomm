let BrandSearchPipe = class BrandSearchPipe {
    transform(brands, args) {
        let searchText = new RegExp(args, 'ig');
        if (brands) {
            return brands.filter(brand => {
                if (brand.name) {
                    return brand.name.search(searchText) !== -1;
                }
            });
        }
    }
};
