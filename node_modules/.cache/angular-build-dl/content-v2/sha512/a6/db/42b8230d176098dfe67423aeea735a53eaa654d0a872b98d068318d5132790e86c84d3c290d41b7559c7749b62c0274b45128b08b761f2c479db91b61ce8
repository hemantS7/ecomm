    checkStatus(status) {
        if (status == "200") {
            this.status_data = {
                message: "OK",
                errorcode: 200
            };
        }
        else if (status == "400") {
            this.status_data = {
                message: "Bad Request",
                errorcode: 400
            };
        }
        else if (status == "401") {
            this.status_data = {
                message: "Unauthorized",
                errorcode: 401
            };
            this.logout();
            return this.status_data;
        }
        else if (status == "403") {
            this.status_data = {
                message: "Forbidden",
                errorcode: 403
            };
        }
        else if (status == "404") {
            this.status_data = {
                message: "Not Found",
                errorcode: 404
            };
        }
        else if (status == "500") {
            this.status_data = {
                message: "Internal Server Error",
                errorcode: 500
            };
        }
        else if (status == "201") {
            this.status_data = {
                message: "Created",
                errorcode: 201
            };
        }
        else if (status == "409") {
            this.status_data = {
                message: "conflict",
                errorcode: 409
            };
        }
        return this.status_data;
